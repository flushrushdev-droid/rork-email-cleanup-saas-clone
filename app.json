{
  "expo": {
    "name": "Email Cleanup SaaS Clone",
    "slug": "email-cleanup-saas-e0pgo27u",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "athenxmail-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.athenxmail.email-cleanup-saas",
      "infoPlist": {
        "NSUserActivityTypes": [
          "INShowInboxIntent",
          "INSendMessageIntent"
        ],
        "NSUserActivityDomain": "app.athenxmail.email-cleanup-saas",
        "NSSupportsLiveActivities": true,
        "NSFaceIDUsageDescription": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "usesIcloudStorage": true,
      "entitlements": {
        "com": {
          "apple": {
            "developer": {
              "networking": {
                "wifi-info": true
              }
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.athenxmail.email_cleanup_saas",
      "intentFilters": [
        {
          "action": "android.intent.action.SEND",
          "category": [
            "android.intent.category.DEFAULT"
          ],
          "data": {
            "mimeType": "text/plain"
          }
        },
        {
          "action": "android.intent.action.SEND",
          "category": [
            "android.intent.category.DEFAULT"
          ],
          "data": {
            "mimeType": "image/*"
          }
        },
        {
          "action": "android.intent.action.SEND",
          "category": [
            "android.intent.category.DEFAULT"
          ],
          "data": {
            "mimeType": "*/*"
          }
        },
        {
          "action": "android.intent.action.VIEW",
          "category": [
            "android.intent.category.DEFAULT",
            "android.intent.category.BROWSABLE"
          ],
          "data": {
            "scheme": "athenxmail-app"
          }
        }
      ],
      "shortcuts": [
        {
          "icon": "./assets/images/adaptive-icon.png",
          "shortcutId": "unread",
          "shortcutLabel": "Unread Emails",
          "shortcutLongLabel": "View unread emails",
          "intent": {
            "action": "android.intent.action.VIEW",
            "data": "athenxmail-app://mail?filter=unread"
          }
        },
        {
          "icon": "./assets/images/adaptive-icon.png",
          "shortcutId": "inbox",
          "shortcutLabel": "Inbox",
          "shortcutLongLabel": "Open inbox",
          "intent": {
            "action": "android.intent.action.VIEW",
            "data": "athenxmail-app://mail"
          }
        },
        {
          "icon": "./assets/images/adaptive-icon.png",
          "shortcutId": "sync",
          "shortcutLabel": "Sync Emails",
          "shortcutLongLabel": "Sync your email inbox",
          "intent": {
            "action": "android.intent.action.VIEW",
            "data": "athenxmail-app://mail?action=sync"
          }
        }
      ],
      "permissions": [
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "cc5e912e-35dd-40d3-95a7-103db22eef0b"
      }
    }
  }
}
